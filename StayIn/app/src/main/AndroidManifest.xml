<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.karthik.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DisplayMessageActivity" />
        <activity
            android:name=".Checkin"
            android:label="@string/title_activity_checkin" />
        <activity android:name=".confirmation" />
        <activity
            android:name=".customerdetails"
            android:label="@string/title_activity_customerdetails"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".details"></activity>
    </application>

</manifest>